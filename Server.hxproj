  <postBuildCommand alwaysRun="False">$(ProjectDir)/tools/win/run.bat</postBuildCommand>
  <!-- Other project options -->
  <options>
    <option showHiddenPaths="False" />
    <option testMovie="Webserver" />
    <option testMovieCommand="$(ProjectDir)/deploy/client/index.html" />
  </options>
  <!-- Plugin storage -->
  <storage />
